{
  "manifest_version": 3,
  "name": "Checkout+ Button Injector",
  "version": "1.0",
  "description": "Replaces the Shopify checkout button text with adjusted total including our fee.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}